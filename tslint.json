{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended"
	],
	"jsRules": {},
	"rules": {
		"no-console": false,
		"no-trailing-whitespace": false,
		"max-classes-per-file": false,
		"max-line-length": false,
		"max-file-line-count": false,
		"arrow-parens": [
			true,
			"ban-single-arg-parens"
		],
		"quotemark": [
			true,
			"single"
		],
		"indent": [
			true,
			"tabs",
			4
		],
		"trailing-comma": [
			true,
			{
				"multiline": "always",
				"singleline": "never"
			}
		],
		"prefer-const": true,
		"triple-equals": true,
		"no-require-imports": true,
		"eofline": false,
		"comment-format": false
	},
	"rulesDirectory": []
}